<Window x:Class="S502.EventPointWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:S502"
        mc:Ignorable="d"
        Title="EventPointWindow" Height="284.469" Width="369.098" ResizeMode="NoResize">
    <Grid>
        <TextBlock x:Name="Timestamp" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="343" Height="25"/>
        <TextBlock x:Name="Description" HorizontalAlignment="Left" Margin="10,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="343" Height="25"/>
        <TextBox x:Name="Detail" Height="139" Margin="10,70,10,0" TextWrapping="Wrap" VerticalAlignment="Top" RenderTransformOrigin="0.282,0.942"/>
        <Button x:Name="Save" Content="保存" HorizontalAlignment="Left" Margin="10,214,0,10" Width="126" Click="Save_Click"/>
        <Button x:Name="Cancel" Content="取消" Margin="231,214,10,10" IsDefault="True" Click="Cancel_Click"/>

    </Grid>
</Window>
